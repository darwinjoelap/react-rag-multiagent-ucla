# Application
APP_NAME=RAG Multiagent System
APP_VERSION=1.0.0
DEBUG=True
ENVIRONMENT=development

# API
API_HOST=0.0.0.0
API_PORT=8000
API_PREFIX=/api/v1

# CORS
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2:latest

# ChromaDB
CHROMA_PERSIST_DIRECTORY=../data/vectorstore
CHROMA_COLLECTION_NAME=ucla_documents

# Document Processing
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
MAX_DOCUMENTS=1000

# Embeddings
EMBEDDING_MODEL=sentence-transformers/paraphrase-multilingual-MiniLM-L12-v2

# Agent Configuration
MAX_ITERATIONS=5
AGENT_TIMEOUT=300

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/app.log